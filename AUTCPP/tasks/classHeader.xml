    <path id="AUTCPP.classpath">
        <path refid="AUTCPP.libraryclasspath"/>
    </path>

    <!-- The classpath with only the jars available in the plugin folder of Eclipse -->
    <path id="AUTCPP.libraryclasspath.alljars">
        <pathelement location="../AUTCPP/bin"/>
        <fileset dir="${ECLIPSE_HOME}/plugins/" includes="*.jar"/>
    </path>

    <path id="AUTCPP.classpath.alljars">
        <path refid="AUTCPP.libraryclasspath.alljars"/>
    </path>

    <!-- The default task with the jars used by this project -->
    <target name="classHeader">
        <java classname="AUTCPP.services.ClassHeader" classpathref="AUTCPP.classpath">
            <arg value="${model}"/>
            <arg value="${target}"/>
        </java>
    </target>

    <!--
        In case of problem you can also try with all the jar available in your Eclipse
        Keep in mind that having two versions of the same jar can create problems
    -->
    <target name="classHeader.alljars">
        <java classname="AUTCPP.services.ClassHeader" classpathref="AUTCPP.classpath">
            <arg value="${model}"/>
            <arg value="${target}"/>
        </java>
    </target>
